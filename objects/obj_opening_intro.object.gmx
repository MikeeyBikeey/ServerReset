            <string>///initialize

evil_happening = false;
smoke_rising = false;
fire_alpha = 0;
player = instance_create(0, 0, obj_intro_player);;
did_lvl_change = false;

if obj_main.lvl == 0 {
    scr_music_change(snd_song_good_and_needy);

    alarm[0] = scr_seconds(3);
    alarm[1] = scr_seconds(10);
    player.x = -4;
    player.y = obj_server_opening.y;
    player.alarm[0] = scr_seconds(15);
    with instance_create(room_width / 2 - 128, -8, obj_intro_endboss) {
        var in_time = scr_seconds(3);
        alarm[0] = in_time;
        image_xscale = 0;
        image_yscale = 0;
        scr_tween(array("x", room_width / 2, "y", 50, "image_xscale", 1, "image_yscale", 1), in_time, scr_void);
    }
    
} else {
    var boss = instance_create(room_width / 2, 50, obj_intro_endboss);
    evil_happening = true;
    smoke_rising = true;
    fire_alpha = 0.2;
    
    // create smoke
    repeat 100 {
        instance_create(irandom(room_width), irandom(room_height), obj_intro_smoke);
    }
    
    // make things evil and villagers panic
    with obj_server_opening {
        if lvl &lt; obj_main.lvl {
            evilness = 0;
        } else if lvl == obj_main.lvl {
            evilness = 1;
            scr_tween(array("evilness", 0), scr_seconds(5), scr_void);
        } else {
            evilness = 1;
        }
    }
    
    with obj_intro_bot {
        evilness = 1;
    }
    
    with obj_intro_villager {
        alarm[1] = 1;
    }
    
    // move intro player
    with obj_server_opening {
        if lvl == obj_main.lvl {
            other.player.x = x;
            other.player.y = y;
        }
    }
    player.alarm[0] = scr_seconds(3);
    
    if obj_main.lvl == 3 {
        boss.alarm[1] = scr_seconds(15);
    
        alarm[2] = scr_seconds(3);
        player.target = instance_nearest(player.x, player.y, obj_intro_player_endpos);
        player.alarm[0] = -1;
        player.image_xscale = -1;
        
        // make villagers happy
        with obj_intro_villager {
            alarm[2] = scr_seconds(8);
        }
        
        scr_music_change(snd_song_gnomes_house);
    } else {
        scr_music_change(snd_song_evils_play);
    }
}

with obj_server_opening {
    if lvl == obj_main.lvl + 1 {
        other.player.target = id;
    }
}

