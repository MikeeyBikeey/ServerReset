            <string>///update state

speed = 0;
switch state {
case STATE_DEATH_RING:
    var server = instance_nearest(x, y, obj_server);
    if server != noone {
        direction -= angle_difference(direction, point_direction(x, y, server.x, server.y)) / 16;
        speed = 8;
        if point_distance(x, y, server.x, server.y) &gt; 200 {
            break;
        }
    }

    if alarm[0] == -1 {
        alarm[0] = death_ring_time;
    }
    
    if was_hit {
        state = STATE_HIDE;
    }
    break;
    
case STATE_RING_OF_FIRE:
    state = STATE_CHASE_TARGET;
    if target != noone {
        for(var i = 0; i &lt; 80; i++) {
            var bomb = instance_create(
                target.x + lengthdir_x(150, (360 / 80) * i),
                target.y + lengthdir_y(150, (360 / 80) * i),
                obj_fire_bomb);
            bomb.speed = 0;
            bomb.height = 100;
            bomb.cur_fall_spd = -1;
        }
    }
    break;
    
case STATE_RAINING:
    if alarm[1] == -1 {
        alarm[1] = rain_drop_time;
    }
    cur_rain_time += 1;
    if cur_rain_time &gt; rain_time {
        state = STATE_CHASE_TARGET;
        cur_rain_time = 0;
        rain_drops = 0;
    }
    break;
    
case STATE_CHASE_TARGET:
    if target != noone {
        direction -= angle_difference(direction, point_direction(x, y, target.x, target.y)) / 32;
        speed = 8;
    }
    break;
    
case STATE_HIDE:

    if target != noone {
        direction -= angle_difference(direction, point_direction(x, y, target.x, target.y - 500)) / 8;
        speed = 8;
    }
    break;
    
default:
    break;
}

