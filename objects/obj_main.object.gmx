        <arguments>
          <argument>
            <kind>1</kind>
            <string>///push all pushables

for(var i = 0; i &lt; num_pushable_near; i++) {
    for(var j = 0; j &lt; num_pushable_near; j++) {
        if pushable_near[j] == pushable_near[i]
            || !instance_exists(pushable_near[j])
            || !instance_exists(pushable_near[i])
        {
            continue;
        }
        
        if point_distance(pushable_near[j].x, pushable_near[j].y, pushable_near[i].x, pushable_near[i].y) &lt; 16 {
            var dir = point_direction(pushable_near[i].x, pushable_near[i].y, pushable_near[j].x, pushable_near[j].y);
            pushable_near[j].x += lengthdir_x(1, dir);
            pushable_near[j].y += lengthdir_y(1, dir);
        }
    }
}

</string>
          </argument>
        </arguments>
      </action>
