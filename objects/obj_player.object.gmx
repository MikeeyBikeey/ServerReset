            <string>///movement and booster

var ms_dir = point_direction(x, y, mouse_x, mouse_y);
var ang_dif = sign(angle_difference(direction, ms_dir));
direction -= ang_dif * booster_turn_spd;

sprite_index = spr_player;

if keyboard_check(vk_space) &amp;&amp; obj_main.has_booster {
    speed = booster_spd;
    sprite_index = spr_player_walk;
    
    var dust = instance_create(x, y, obj_hammer_pow);
    dust.direction = direction - 180;
    
    image_xscale = sign(lengthdir_x(1, direction));
    if image_xscale == 0 {
        image_xscale = 1;
    }
    
} else {
    speed = 0;
    
    if keyboard_check(ord('A')) {
        x -= run_speed;
        image_xscale = -1;
        sprite_index = spr_player_walk;
    }
    if keyboard_check(ord('D')) {
        x += run_speed;
        image_xscale = 1;
        sprite_index = spr_player_walk;
    }
    if keyboard_check(ord('W')) {
        y -= run_speed;
        sprite_index = spr_player_walk;
    }
    if keyboard_check(ord('S')) {
        y += run_speed;
        sprite_index = spr_player_walk;
    }
}

</string>
